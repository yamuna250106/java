import java.io.*;
import java.util.*;
import java.text.*;
import java.math.*;
import java.util.regex.*;

public class Solution {

    public static void main(String[] args) {
        Scanner sc=new Scanner(System.in);
        int a=sc.nextInt();
        int b=sc.nextInt();
         int c=sc.nextInt();
        int d=sc.nextInt();
        int e=sc.nextInt();
        int num1=a+b+c+d+e;
        float num2=(float)num1/5;
        System.out.println("Sum of the 5 number is : "+num1);
        System.out.printf("Average of the 5 number is : "+num2);
    }
}
